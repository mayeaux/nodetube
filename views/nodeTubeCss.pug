    .preview-image {
        width: inherit;
        max-width: 405px;
        height: 210px;
        max-height: 210px;
        border-radius: 7px;
        max-width:100%;
        /*border:1px solid white;*/
    }

    /* translate thing to make it centered even with overflow*/
    @media (max-width: 347px) {
        .preview-image {
            max-width: 280px;
            margin-left: 50%;
            transform: translateX(-50%);
        }
    }

    @media (min-width: 900px) {
        .navbar-left {
            float: left !important;
        }

        .navbar-right {
            float: right !important;
            margin-right: -95px;
        }

        .navbar-right ~ .navbar-right {
            margin-right: 0;
        }
    }

    @media (min-width: 1042px) {
        .navbar-right {
            margin-right: -28px;
        }
    }

