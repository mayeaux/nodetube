//- doing this because otherwise it's too annoying with cloudflare caching
style.

    @import url('https://rsms.me/inter/inter.css');

    html {
        font-family: 'Inter', sans-serif;
    }

    @supports (font-variation-settings: normal) {
        html {
            font-family: 'Inter var', sans-serif;
        }
    }

    .navbar-default .navbar-nav > li > a {
        color: white;
    }

    .view-header {
        display: inline;
    }

    /* 70 : 39 aspect ratio */

    .border-preview {
        border: 1px solid black;
    }

    .upload-title {
        height: 89px;
        overflow: hidden;
        display: flex;
        display: -webkit-flex;
        width: 80%;
        margin: 0 auto;
    }

    .title-anchor:hover {
        text-decoration: none;
        color: red;
    }

    .title-anchor {
        text-decoration: none;
    }

    .upload-title-text {
        max-height: 53px;
        color: #bcbcbc;
        font-size: 20px;
        text-align: center;
        margin: 0;
        margin-top: 8px;
        overflow: hidden;

        font-weight: 300;
    }

    .upload-title-text:hover {
        text-decoration: none
        /*color:black;*/
    }

    .upload-user-text {
        color: #c7c7c7 !important;
        /*text-decoration:underline;*/
    }

    .upload-user-text:hover {
        color: black;
        /*text-decoration:underline;*/
    }

    .search-upload-buttons-container {
        display: grid;
        justify-content: center;
        grid-gap: 10px;
    }

    @media (min-width: 480px) {
        .search-upload-buttons-container {
            grid-auto-flow: column;
        }
    }

    .twenty-six {
        font-size: 26px;
    }

    .upload-details {
        width: 90%;
        margin: 0 auto;
        margin-top: -12px;
    }

    .views {
        height: 30px;
        display: flex;
        display: -webkit-flex;
    }

    .views-text {
        font-size: 14px;
        text-align: center;
        margin: auto;
        color: rgba(0, 0, 0, 0.7);
    }

    .upload-file {
        margin-right: 167px;
        margin-bottom: 20px;
        width: 490px;
    }

    .change-size-button {
        display: inline-block;
        margin: 10px;
    }

    .unclicked-emoji img {
        opacity: 0.5;
    }

