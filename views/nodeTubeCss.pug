    .upload-thumbnail {
        display: block;
    }

    .image-container {
        display:inline-block;
        border:1px solid #5b5a5a;
        margin:0 auto;
        overflow:hidden;
        border-radius:13px;
    }



    .upload-thumbnail:hover {

        /*transform: scale(1.05) translate(0px, 30px);*/

        transform: scale(1.12);

        -webkit-filter: brightness(120%);
    }

    .upload-thumbnail {
        transition: transform .7s ease;
